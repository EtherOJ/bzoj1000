start
     a=1 d=2
     b=10 c=2
     loop 10
          a=a+1
          c=c+1
          d=d+1
          b=(a+d)*c+(a+b/2)
          loop 3
               b=b/8
               break
          end
     end
     loop 20
          a=a+2 c=c+3
          d=d+1
          b=(a+d)*c+(a+b)*1-3
          loop 2
               b=b/8
               continue
          end
     end
     write b
     t=0
     loop 100
          t=t+b
     end
     t=t/100
     write t
end
